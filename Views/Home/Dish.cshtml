

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <header>
        <header>
            <h1>@Model.Name </h1>
        </header>
        Checkout some recent dishes! <a href="/new">link</a>
    </header>
    <div>
        <table>
            <thead>
                <th>Dishes</th>
            </thead>
            <tbody>
                @model Dish
            
                <tr>
                    <td>@Model.Name </td>
                    <td>@Model.Chef </td>
                    <td>@Model.Tastyness </td>
                    <td>@Model.Calories </td>
                    <td>@Model.UpdatedAt </td>

                    
    
                </tr>
                
            

            </tbody>
        </table>
    </div>

    
</body>
</html>